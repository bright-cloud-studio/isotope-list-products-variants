{
    "name":"bright-cloud-studio/isotope-list-products-variants",
    "description":"A custom Product List which also lists variants",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["contao", "isotope", "list", "product", "products", "variant", "variants"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "require":{
        "php": "^8.0",
        "contao/core-bundle": "^4.13",
        "isotope/isotope-core": ">=2.6"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/isotope_list_product_variants/library/ListProductVariants"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/isotope_list_product_variants": "system/modules/isotope_list_product_variants"
            }
        }
    },
    "minimum-stability": "dev"
}
